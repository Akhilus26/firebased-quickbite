{
  "expo": {
    "name": "Quick Bite",
    "slug": "quickbite",
    "scheme": "quickbite",
    "version": "0.1.0",
    "orientation": "portrait",
    "plugins": [
      "expo-router",
      "expo-asset",
      "expo-secure-store"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "android": {
      "package": "com.quickbite.app",
      "newArchEnabled": true
    },
    "ios": {
      "bundleIdentifier": "com.quickbite.app",
      "newArchEnabled": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "17c8a44a-3a4a-4d59-82c5-629f87879b4b"
      }
    }
  }
}
